<% blocks.title=__(path) %>
    <div class="row heading-bg bg-primary">
        <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
            <h5 class="txt-light"><%= __(path) %></h5>
        </div>
        <!-- Breadcrumb -->
        <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
            <ol class="breadcrumb">
                <li><a href="/admin"><span><%= __('Admin') %></span></a></li>
                <li class="active"><span><%= __(path) %></span></li>
            </ol>
        </div>
        <!-- /Breadcrumb -->
    </div>
    <div class="app-icon-wrap">
        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-default card-view">
                    <!-- <div class="wrapper"> -->
                    <turboRender module="Admin/*" />
                    <script type="text/javascript">
                        window.___definition = <%- JSON.stringify(sails.models[path]._attributes) %>;
                    </script>
                </div>
            </div>
        </div>
    </div>
